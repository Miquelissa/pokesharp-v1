<Window x:Class="Pokesharp.EditClient"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pokesharp"
        mc:Ignorable="d"
        Title="EditClient" Height="291" Width="305">
    <Grid Margin="-1,53,1,-53">
        <Label x:Name="Perfil" Content="Perfil" HorizontalAlignment="Left" Margin="124,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="nickname" HorizontalAlignment="Left" Height="24" Margin="20,80,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="255"/>
        <Button x:Name="save" Click="save_Click" Content="Save" HorizontalAlignment="Left" Margin="200,140,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="1.387,0.35"/>
        <Button x:Name="exit" Click="exit_Click" Content="Exit" HorizontalAlignment="Left" Margin="96,140,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="1.387,0.35"/>
        <Label Content="Nickname:" HorizontalAlignment="Left" Margin="20,49,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.159,-0.769"/>

    </Grid>
</Window>
